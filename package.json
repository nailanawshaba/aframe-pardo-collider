{
  "name": "aframe-pardo-collider-component",
  "version": "1.0.0",
  "description": "A simple collider for detecting when two objects interact with each other",
  "main": "index.js",
  "unpkg": "dist/aframe-pardo-collider-component.min.js",
  "scripts": {
    "dev": "budo index.js:dist/aframe-aframe-pardo-collider-component-component.min.js --port 7000 --live --open",
    "lint": "semistandard -v | snazzy",
    "prepublish": "npm run",
    "ghpages": "ghpages",
    "start": "npm run dev",
    "test": "karma start ./tests/karma.conf.js",
    "test:firefox": "karma start ./tests/karma.conf.js --browsers Firefox",
    "test:chrome": "karma start ./tests/karma.conf.js --browsers Chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pardolab/aframe-pardo-collider.git"
  },
  "keywords": [
    "aframe",
    "aframe-component",
    "aframe-vr",
    "vr",
    "mozvr",
    "webvr",
    "aframe-pardo-collider-component"
  ],
  "author": "Pardo Lab <contact@pardolab.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pardolab/aframe-pardo-collider/issues"
  },
  "homepage": "https://github.com/pardolab/aframe-pardo-collider#readme",
  "devDependencies": {
    "@types/three": "^0.84.28",
    "aframe": "*",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-esnext": "^1.1.3",
    "browserify": "^13.0.0",
    "budo": "^8.2.2",
    "chai": "^3.4.1",
    "chai-shallow-deep-equal": "^1.3.0",
    "ghpages": "^0.0.8",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-typescript": "^3.2.3",
    "gulp-uglify": "^3.0.0",
    "install": "^0.10.1",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-chai-shallow-deep-equal": "0.0.4",
    "karma-chrome-launcher": "2.0.0",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-sinon-chai": "^1.1.0",
    "mocha": "^2.3.4",
    "randomcolor": "^0.4.4",
    "semistandard": "^8.0.0",
    "shelljs": "^0.7.0",
    "shx": "^0.1.1",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "snazzy": "^4.0.0",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.3.0",
    "typescript": "^2.6.1"
  },
  "semistandard": {
    "globals": [
      "AFRAME",
      "THREE"
    ],
    "ignore": [
      "examples/build.js",
      "dist/**"
    ]
  },
  "dependencies": {
    "pump": "^1.0.2",
    "uglify": "^0.1.5"
  }
}
